{
  "functions": {
    "api/saved-itineraries/*/refresh": {
      "maxDuration": 60
    },
    "api/gemini/itinerary-generator": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
